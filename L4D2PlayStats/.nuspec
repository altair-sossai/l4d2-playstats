<?xml version="1.0" encoding="utf-8"?>

<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>L4D2PlayStats</id>
        <version>1.0.0.11</version>
        <title>Left 4 dead 2 statistics parser</title>
        <description>
            Allows extracting existing data from statistics files
            generated by the l4d2_playstats.smx plugin
        </description>
        <authors>altairsossai@gmail.com</authors>
        <owners>altairsossai</owners>
        <projectUrl>https://github.com/altair-sossai/l4d2-playstats</projectUrl>
        <icon>images\icon.png</icon>
        <readme>docs\README.md</readme>
        <frameworkReferences>
            <group targetFramework="net6.0" />
        </frameworkReferences>
    </metadata>
    <files>
        <file src="..\assets\icon.png" target="images" />
        <file src="..\README.md" target="docs" />
    </files>
</package>